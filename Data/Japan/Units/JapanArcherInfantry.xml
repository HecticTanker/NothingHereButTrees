	<!-- A.K.A. Archer Maiden-->
	<GameObject
		id="JapanArcherInfantry"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_ArcherMaiden"
		ButtonImage="Portrait_ArcherMaiden"
		Side="Allies"
		SubGroupPriority="420" 
		EditorSorting="UNIT"	
		BuildTime="5"
		CommandSet="JapanArcherInfantryCommandSet"
		KindOf="SELECTABLE CAN_ATTACK CAN_CAST_REFLECTIONS INFANTRY SCORE"
		ThreatLevel="10"
		WeaponCategory="MISSILE"
		VoicePriority="100"
		EditorName="JapanArcherInfantry"
		Description="Desc:JapanArcherInfantry"
		TypeDescription="Type:JapanArcherInfantry"
		UnitTypeIcon="JavelinIcon"
		HealthBoxHeightOffset="30">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:JapanArcherInfantry</DisplayName>
		<ObjectResourceInfo>
			<BuildCost Account="=$ACCOUNT_ORE" Amount="400"/>
		</ObjectResourceInfo>
		<ArmorSet
			Armor="AlliedAntiVehicleInfantryArmor"
			DamageFX="InfantryDamageFX" />
		<LocomotorSet
			Locomotor="TestReactiveLocomotorHUMAN"
			Condition="NORMAL"
			Speed="50" />
		<SkirmishAIInformation
			UnitBuilderStandardCombatUnit="true" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model Name="JUArcherInfantry_SKN" />
					<WeaponFireFXBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="ARROW" />
					<WeaponLaunchBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="ARROW" />
					<WeaponFireFXBone
						WeaponSlotID="1"
						WeaponSlotType="SECONDARY_WEAPON"
						BoneName="ARROW" />
					<WeaponLaunchBone
						WeaponSlotID="1"
						WeaponSlotType="SECONDARY_WEAPON"
						BoneName="ARROW" />
					<WeaponFireFXBone
						WeaponSlotID="2"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="ARROW" />
					<WeaponLaunchBone
						WeaponSlotID="2"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="ARROW" />
          
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model Name="" />
				</ModelConditionState>
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Model Name="NPSkeleton_SKN" />
				</ModelConditionState>
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Model Name="NPRadiation" />
						<ParticleSysBone
							BoneName="None"
							FXParticleSystemTemplate="RadiationPuddle"
							FollowBone="true" />
					    <ParticleSysBone
							BoneName="None"
							FXParticleSystemTemplate="RadiationSplash"
							FollowBone="true" />
					    <ParticleSysBone
							BoneName="None"
							FXParticleSystemTemplate="RadiationLightSmall"
							FollowBone="true" />          
				</ModelConditionState>
				
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_bored"
					AllowRepeatInRandomPick="true"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="JUArcherInfantry_BIDA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUArcherInfantry_BIFA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="JUArcherInfantry_BIFB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_Suppressed" then
						CurDrawableSetTransitionAnimState("TRANS_fromSuppressed")
						elseif Prev == "STATE_Parachuting" then
						CurDrawableSetTransitionAnimState("TRANS_fromParachuting")
						end
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="JUArcherInfantry_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="JUArcherInfantry_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
			
				<!-- meta impact anims -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="JUArcherInfantry_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING SPLATTED">
					<Animation
						AnimationName="JUArcherInfantry_LNDA"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_STANDING_UP">
					<Animation
						AnimationName="JUArcherInfantry_GTPA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED">
					<Animation
						AnimationName="JUArcherInfantry_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING OVER_WATER"
					StateName="STATE_OverWater">
					<Animation
						AnimationName="JUArcherInfantry_SWMB"
						AnimationMode="LOOP" />
						<ParticleSysBone
							BoneName="None"
							FXParticleSystemTemplate="InfantryWaterWake"
							FollowBone="false" />          
						<Script>
							Prev = CurDrawablePrevAnimationState()
							if Prev ~= "STATE_OverWater" and Prev ~= "TRANS_ToWater"  then
							CurDrawableSetTransitionAnimState("TRANS_ToWater")
							end
						</Script>
					</AnimationState>
					
				<!-- parachuting anims -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="LAUNCHED"
					StateName="STATE_ParachuteLaunch">
					<Animation
						AnimationName="JUArcherInfantry_MCNA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="PARACHUTING"
					StateName="STATE_Parachuting">
					<Animation
						AnimationName="JUArcherInfantry_MCNC"
						AnimationMode="LOOP" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_ParachuteLaunch" then
						CurDrawableSetTransitionAnimState("TRANS_toParachuting")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_toParachuting" >
					<Animation
						AnimationName="JUArcherInfantry_MCNB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromParachuting" >
					<Animation
						AnimationName="JUArcherInfantry_MCND"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED DYING"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SDIA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- radiation death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5 OVER_WATER">
					<Animation
						AnimationName="NPRadiation_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- radiation death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Animation
						AnimationName="NPRadiation_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- sword death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_4">
					<Animation
						AnimationName="JUArcherInfantry_DTBA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUArcherInfantry_DTBB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- fire death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_3">
					<Animation
						AnimationName="JUArcherInfantry_DTBA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUArcherInfantry_DTBB"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="InfantryFireDeath"
						FollowBone="true" />
				</AnimationState>


				<!-- prism/laser death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_2">
					<Animation
						AnimationName="JUArcherInfantry_DTBA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUArcherInfantry_DTBB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- tesla death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Animation
						AnimationName="NPSkeleton_DIEA"
						AnimationMode="ONCE" />
						<ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="Soviet_TeslaDeath_Spark"
						FollowBone="true" />
				</AnimationState>
				
				<!-- dying when running -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING_WASMOVING">
					<Animation
						AnimationName="JUArcherInfantry_RUNDA"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- default death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="JUArcherInfantry_DTBA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUArcherInfantry_DTBB"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP">
					<Animation
						AnimationName="AUAntiVehicleInfantry_CWRY"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_CWRZ"
						AnimationMode="LOOP" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStun01"
							FollowBone="true" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStunGlows01"
							FollowBone="true" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED MOVING"
					StateName="STATE_Suppressed"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="JUArcherInfantry_SMVA"
						AnimationMode="LOOP" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED USING_WEAPON_A"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SATA"
						AnimationMode="LOOP" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="JUArcherInfantry_SIDA"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev ~= "STATE_Suppressed" and Prev ~= "TRANS_toSuppressed" then
						CurDrawableSetTransitionAnimState("TRANS_toSuppressed")
						end
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="JUArcherInfantry_RUNA"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="40" />
					<Animation
						AnimationName="JUArcherInfantry_RUNB"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="40" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_OverWater" or Prev == "TRANS_ToWater" then
						CurDrawableSetTransitionAnimState("TRANS_FromWater")
						end
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FIRING_OR_PREATTACK_A"
					FrameForPristineBonePositions="1">
					<Animation
						AnimationName="JUArcherInfantry_ATKA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"/>
					<Animation
						AnimationName="JUAntiVehicleInfantry_ATKB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"/>
					<Animation
						AnimationName="JUAntiVehicleInfantry_ATKC"
						AnimationMode="ONCE"
						AnimationBlendTime="15"/>
					<Animation
						AnimationName="JUArcherInfantry_SATKA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"/>	
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ATTACKING"
					Flags="RESTART_ANIM_WHEN_COMPLETE" 
					StateName="STATE_SelectedPose">
					<Animation
						AnimationName="JUArcherInfantry_AIDA"
						AnimationBlendTime="10"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_toSuppressed">
					<Animation
						AnimationName="JUArcherInfantry_SDVA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromSuppressed"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="JUArcherInfantry_SGTA"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_boredToAttention">
					<Animation
						AnimationName="JUArcherInfantry_BATA"
						AnimationMode="ONCE" 
						AnimationBlendTime="15"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_attentionToBored">
					<Animation
						AnimationName="JUArcherInfantry_ABTA"
						AnimationMode="ONCE" 
						AnimationBlendTime="15"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ENGAGED OVER_WATER">
					<Animation
						AnimationName="JUArcherInfantry_WAIDA"
						AnimationMode="LOOP" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="SmallShipWakeIdle"
						FollowBone="false" />          
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="OVER_WATER"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_OverWater">
					<Animation
						AnimationName="JUArcherInfantry_WAIDA"
						AnimationMode="LOOP" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="SmallShipWakeIdle"
						FollowBone="false" />          
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_ToWater">
					<Animation
						AnimationName="JUArcherInfantry_SWMA"
						AnimationMode="ONCE" />
					<Script>
						CurDrawablePlaySound("SOV_Commando_WaterSplash")
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_FromWater">
					<Animation
						AnimationName="JUArcherInfantry_SWMC"
						AnimationMode="ONCE" />
				</AnimationState>
				
			</ScriptedModelDraw>

			<!-- include parachute draw module. -->
			<xi:include
				href="DATA:Includes/ParachuteDrawJapan.xml"
				xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:ParachuteDraws/child::*)" />
			
		</Draws>
		<Behaviors>
			<WeaponSetUpdate
				id="ModuleTag_WeaponSetUpdate">
				<WeaponSlotHardpoint
					ID="1">
					<Weapon
						Ordering="PRIMARY_WEAPON"
						Template="JapanArcherInfantryArrow"/>
				</WeaponSlotHardpoint>
				<WeaponSlotHardpoint
					ID="2">
					<Weapon
						Ordering="PRIMARY_WEAPON"
						Template="JapanArcherInfantryArrow_Aircraft"/>
				</WeaponSlotHardpoint>
			</WeaponSetUpdate>
			
			<AimWeapon
				id="ModuleTag_AimWeapon" />
			<Physics
				id="ModuleTag_Physics" />
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="3s"
				SinkRate="2.4"
				DestructionDelay="4.5s">
				<DieMuxData
					DeathTypes="ALL" 
					DeathTypesForbidden="ELECTROCUTED LASERED BURNED STABBED IRRADIATED"/>
				<Sound
					Type="INITIAL"
					List="IUArche_voiDie" />
			</SlowDeath>

			<SlowDeath
				id="ModuleTag_TeslaDeath"
				DeathFlags="DEATH_1"
				SinkDelay="7s"
				SinkRate="2.4"
				DestructionDelay="8.5s">
                <DieMuxData
					DeathTypes="ELECTROCUTED"
					DeathTypesForbidden="LASERED BURNED STABBED IRRADIATED"/>
				<Sound Type="INITIAL" List="IUArche_voiDieShock" />
			</SlowDeath>
			
			<SlowDeath
				id="ModuleTag_LaserDeath"
				DeathFlags="DEATH_2"

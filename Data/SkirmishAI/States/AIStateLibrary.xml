  <!--
  If the AI's MCV is under heavy attack the AIEmergencyManager will tell it to pack up.  Then this state will grab it over the JapanEggUnpacker
  and send it to an open expansion to unpack at.
  -->
  <AIStrategicStateDefinition
    id="RetreatMCVToLand"
    MaxRunningCopies="1">
    <Heuristic>
      <ObjectOfTypeExistsHeuristic
        PassIfExists="true"
        Location="Water">
        <ObjectFilter
					Relationship="SAME_PLAYER"
					Rule="ANY" >
          <IncludeThing>AlliedMCV</IncludeThing>
          <IncludeThing>SovietMCV</IncludeThing>
          <IncludeThing>JapanMCV</IncludeThing>
        </ObjectFilter>
      </ObjectOfTypeExistsHeuristic>
    </Heuristic>
    <TargetHeuristic TargetHeuristic="SuperSafeLandExpansionHeuristic" Priority="70"/>
    <TargetHeuristic TargetHeuristic="VerySafeLandExpansionHeuristic" Priority="60"/>
    <TargetHeuristic TargetHeuristic="LandExpansionHeuristic" Priority="50"/>
    <TargetHeuristic TargetHeuristic="SuperSafeWaterExpansionHeuristic" Priority="40"/>
    <TargetHeuristic TargetHeuristic="VerySafeWaterExpansionHeuristic" Priority="30"/>
    <TargetHeuristic TargetHeuristic="WaterExpansionHeuristic" Priority="20"/>
    <TargetHeuristic TargetHeuristic="AnyExpansionHeuristic" Priority="10"/>
    <Tactic id="Expand" Tactic="SimpleExpansion" EndBehavior="DISBAND" Distance="350">
      <TeamTemplate MinUnits="1" MaxUnits="1"
                    RegisteredObjectSet="MCVS"
                    IncludeKindOf="" ExcludeKindOf=""
                    AllowedLocomotorTypes="LAND WATER AMPHIBIOUS AIR"
                    Priority="HIGH" ReleaseType="RELEASE_TO_PRIORITY" AlwaysRecruit="true"
                    MicroManager="FearlessDefenseMicroManager">
        <ObjectFilter Rule="ANY">
          <IncludeThing>AlliedMCV</IncludeThing>
          <IncludeThing>SovietMCV</IncludeThing>
          <IncludeThing>JapanMCV</IncludeThing>
        </ObjectFilter>
      </TeamTemplate>
    </Tactic>
  </AIStrategicStateDefinition>

